{
    "name": "michael-rubel/laravel-model-mapper",
    "description": "Map your model attributes to class properties with ease.",
    "keywords": [
        "michael-rubel",
        "laravel",
        "laravel-model-mapper"
    ],
    "homepage": "https://github.com/michael-rubel/laravel-model-mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^8.67",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.6",
        "brianium/paratest": "^6.3",
        "permafrost-dev/coverage-check": "^1.2",
        "mockery/mockery": "^1.4.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "MichaelRubel\\ModelMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelRubel\\ModelMapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelRubel\\ModelMapper\\ModelMapperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
